{
  "name": "k",
  "version": "1.0.0",
  "description": "--- permalink: README.html copyright: Copyright (c) 2010-2020 K Team. All Rights Reserved. ---",
  "main": "index.html",
  "scripts": {
    "build": "npm run build-css && npm run build-js && npm run build-html",
    "build-css": "parcel build --no-source-maps --no-autoinstall --bundle-node-modules -d ./public_content/assets/css --public-url /assets/css ./static_content/assets/sass/index.scss",
    "build-js": "parcel build --no-source-maps --no-autoinstall --bundle-node-modules -d ./public_content/assets/js --public-url /assets/js ./static_content/assets/js/index.js",
    "build-html": "node build-html.js",
    "serve": "http-server public_content",
    "deploy": "npm run build && gh-pages-clean && node deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kframework/k.git"
  },
  "author": "K | Runtime Verification",
  "license": "UNLICENSED",
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "http-server": "^0.12.3",
    "parcel": "^1.12.4",
    "sass": "^1.26.9",
    "webfonts-generator": "^0.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "animate.css": "^3.7.2",
    "ansi_up": "^4.0.4",
    "bootstrap": "^4.3.1",
    "bootstrap-select": "^1.13.9",
    "clipboard": "2.0.4",
    "imagesloaded": "3.1.8",
    "isotope-layout": "2.2.0",
    "jquery": "^3.5.1",
    "jquery-countdown": "2.2.0",
    "jquery-visible": "^1.2.0",
    "jquery.animate-number": "0.0.11",
    "magnific-popup": "1.0.0",
    "owl.carousel": "2.3.4",
    "popper.js": "^1.16.1",
    "prismjs": "^1.19.0",
    "toastr": "2.1.4",
    "twitter-fetcher": "18.0.2",
    "wow.js": "1.2.2"
  },
  "bugs": {
